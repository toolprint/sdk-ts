{
  "name": "@onegrep/sdk",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "license": "MIT",
  "author": "OneGrep, Inc.",
  "homepage": "https://onegrep.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onegrep/typescript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/onegrep/typescript-sdk/issues"
  },
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "outputs": [
    "dist/**/*"
  ],
  "scripts": {
    "check-types": "tsc -p tsconfig.check.json --noEmit",
    "build:types": "tsc -p tsconfig.types.json",
    "build:cjs": "tsup src/index.ts --tsconfig tsconfig.cjs.json --out-dir dist/cjs --format cjs --dts",
    "build:esm": "tsup src/index.ts --tsconfig tsconfig.esm.json --out-dir dist/esm --format esm --dts",
    "build": "echo 'build success'",
    "licenses": "pnpm licenses list --prod --long > .licenses.txt",
    "package": "mkdir -p pack && pnpm pack --pack-destination pack",
    "test": "LOG_MODE=pino LOG_LEVEL=debug vitest --config vitest.config.ts --disable-console-intercept ",
    "test:watch": "LOG_MODE=pino LOG_LEVEL=debug vitest --config vitest.config.ts --disable-console-intercept --watch",
    "clean:dist": "rimraf ./dist",
    "clean:dist:types": "rimraf ./dist/types",
    "clean:dist:cjs": "rimraf ./dist/cjs",
    "clean:dist:esm": "rimraf ./dist/esm",
    "clean:modules": "rimraf ./node_modules"
  },
  "devDependencies": {
    "@repo/onegrep-api-client": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/utils": "workspace:*",
    "check-licenses": "^1.1.0",
    "dotenv": "^16.4.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6",
    "typescript": "latest",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.5.0",
    "@types/node": "^22.13.4",
    "@zodios/core": "^10.9.6",
    "ajv": "^8.17.1",
    "buffer": "^6.0.3",
    "eventsource": "^3.0.5",
    "@onegrep/json-schema-to-zod": "git+https://github.com/OneGrep/json-schema-to-zod.git#main&path:/packages/json-schema-to-zod",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "dotenv": "^16.4.7"
  }
}
