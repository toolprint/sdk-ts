{
  "name": "@onegrep/sdk",
  "version": "0.0.34",
  "type": "module",
  "license": "MIT",
  "author": "OneGrep, Inc.",
  "homepage": "https://onegrep.dev",
  "bugs": {
    "url": "https://github.com/onegrep/typescript-sdk/issues"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "outputs": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/onegrep/typescript-sdk",
    "directory": "packages/onegrep-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint src package.json",
    "lint:fix": "eslint src package.json --fix",
    "build": "tsup src/index.ts --tsconfig tsconfig.json",
    "test": "LOG_MODE=file LOG_LEVEL=debug vitest run --config vitest.config.ts --disable-console-intercept ",
    "test:watch": "LOG_MODE=file LOG_LEVEL=debug vitest watch --config vitest.config.ts --disable-console-intercept",
    "licenses": "pnpm licenses list --prod --long > .licenses.txt",
    "package": "mkdir -p pack && pnpm pack --pack-destination pack",
    "version": "changeset version",
    "publish:npm": "pnpm publish",
    "publish:npm:dry-run": "pnpm publish --dry-run",
    "clean:dist": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean:pack": "rimraf ./pack"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@onegrep/api-client": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/utils": "workspace:*",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint": "^8.56.0",
    "@types/node": "^22.13.4",
    "chalk": "^5.3.0",
    "check-licenses": "^1.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.9",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-tsc-alias": "^1.1.4",
    "tsup": "^8.3.6",
    "tsx": "^4.19.4",
    "typescript": "latest",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@blaxel/core": "0.2.14",
    "@composio/mcp": "^1.0.5",
    "@dopplerhq/node-sdk": "^1.3.0",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@onegrep/json-schema-to-zod": "1.2.1",
    "@smithery/sdk": "1.4.3",
    "@zodios/core": "^10.9.6",
    "ajv": "^8.17.1",
    "buffer": "^6.0.3",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.10",
    "chalk": "^5.3.0",
    "composio-core": "^0.5.39",
    "date-fns": "^4.1.0",
    "eventsource": "^3.0.5",
    "keyv": "^5.3.3",
    "pkce-challenge": "^5.0.0",
    "uuid": "^11.1.0",
    "yaml": "^2.3.1",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@blaxel/langgraph": "^0.2.0",
    "@hey-api/client-fetch": "^0.10.0",
    "@langchain/core": "^0.3.51",
    "@langchain/langgraph": "^0.2.68",
    "axios": "^1.7.9",
    "@hey-api/client-axios": "^0.8.0"
  }
}
